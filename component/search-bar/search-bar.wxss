/* component/search-bar/search-bar.wxss */

.Rectangle-1 {
  width: 100%;
  height: 50px;
  display: inline-block;
}

.Rectangle-1-1 {
  width:65.8%;
  height: 36px;
  background-size: 247px 36px;
  background-color: #f8f8f8;
  text-align: left;
  display: inline-block;
  vertical-align: top;
  margin-top:5px;
}

.Rectangle-1-1-text {
  font-size: 28rpx;
  color: #333;
  text-align: left;
  display: inline-block;
  vertical-align: top;
  margin-top: 5px;
}

.Rectangle-1-1-text-ph {
  opacity: 0.4;
}

.Rectangle-1-2 {
  display: inline-block;
  width: 50px;
  vertical-align: top;

}

.Symbol-1-1 {
  width: 16px;
  height: 16px;
  display: inline-block;
  vertical-align: top;
  padding: 10px;
}

.Symbol-1-2 {
  width: 24px;
  height: 24px;
  display: inline-block;
  vertical-align: top;
  padding: 10px;
}
